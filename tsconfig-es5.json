{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node16",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "build/es5/",
    "removeComments": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "ES5"
  }
}
