{
  "include": [
    "src",
    "script",
    "test"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "removeComments": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "ES5",
    "types": [
      "mocha",
      "node"
    ]
  }
}
